{
  "name": "@sdsolutions/particle-ng",
  "version": "15.1.0",
  "license": "MIT",
  "description": "Reusable Angular components to use in lieu of Material or PrimeNG",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdsolutionsio/particle-ng"
  },
  "keywords": [
    "angular",
    "components",
    "material",
    "primeng"
  ],
