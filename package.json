  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "eslint": "eslint",
    "e2e": "ng e2e",
    "package": "npm run css-build && ng-packagr -p ng-package.json",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "lint": "ng lint",
    "css-concat": "concat-glob-cli -f ./src/styles/*.css -o ./dist/particle.css",
    "css-minify": "postcss ./dist/particle.css --replace",
    "css-copy": "copyfiles -f ./dist/particle.css ./",
    "css-build": "npm run css-concat && npm run css-minify && npm run css-copy"
  },
